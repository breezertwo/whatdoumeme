{
  "name": "@whatdoumeme/client",
  "version": "1.0.1",
  "author": "breezertwo",
  "license": "GPL-3.0-only",
  "scripts": {
    "clean-dist": "rimraf dist/*",
    "lint": "eslint './src/**/*.{js,ts,tsx}' --quiet",
    "start": "yarn start:dev",
    "start:dev": "set NODE_ENV=development && webpack serve --config=configs/webpack/dev.js",
    "build:prod": "yarn clean-dist && set NODE_ENV=production && webpack --config=configs/webpack/prod.js && yarn post-prod",
    "start:prod": "yarn build:prod && node express.js",
    "post-prod": "cpx \"dist/**\" ../../outDir/public"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.16",
    "@babel/core": "^7.14.0",
    "@babel/plugin-transform-runtime": "^7.14.3",
    "@babel/preset-env": "^7.14.0",
    "@babel/preset-react": "^7.13.13",
    "@babel/preset-typescript": "^7.13.0",
    "@hot-loader/react-dom": "^17.0.1",
    "@types/js-cookie": "^2.2.6",
    "@types/react": "^17.0.4",
    "@types/react-dom": "^17.0.3",
    "@types/react-router-dom": "^5.1.7",
    "babel-loader": "^8.2.2",
    "copy-webpack-plugin": "^8.1.1",
    "cpx": "^1.5.0",
    "css-loader": "^5.2.4",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.3.1",
    "image-webpack-loader": "^7.0.1",
    "mini-css-extract-plugin": "^1.6.0",
    "node-sass": "^5.0.0",
    "react-hot-loader": "^4.13.0",
    "sass-loader": "^11.0.1",
    "style-loader": "^2.0.0",
    "webpack-dev-server": "^3.11.2",
    "webpack-merge": "^5.7.3",
    "webpack-node-externals": "^3.0.0"
  },
  "dependencies": {
    "@material-ui/core": "^4.11.4",
    "@material-ui/icons": "^4.11.2",
    "js-cookie": "^2.2.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.2.0",
    "socket.io-client": "^4.1.1"
  }
}
